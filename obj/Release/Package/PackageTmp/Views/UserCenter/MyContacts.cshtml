@{
    ViewBag.Title = "我的联系人";
    Layout = null;
}
<div class="row">
    <div class="col-md-12">
        <!--联系人列表-->
        <div class="contact-list">
            <div class="contact-header">
                <button class="btn-add"> + 新建联系人</button>
                <div class="header-right">
                    <ul>
                        <li><a id="download">模板下载</a></li>
                        <li><a id="import">批量导入</a></li>
                    </ul>
                </div>
            </div>
            <table class="table table-striped table-no-border contactTable">
                <thead>
                    <tr>
                        <th class="col-md-2">姓名</th>
                        <th class="col-md-2">性别</th>
                        <th class="col-md-3">手机号</th>
                        <th class="col-md-4">身份证号</th>
                        <th class="col-md-1">操作</th>
                    </tr>
                </thead>
                <tbody id="myContactInfo"></tbody>
            </table>

            <div class="pager-actions">
                <div class="pager-desc">总共<b id="contacts_PageCount">0</b> 页,当前为第<b id="contacts_PageIndex">0</b>页</div>
                <ul class="pager-bar">
                    <li>
                        <a class="prev" id="contacts_Prev"></a>
                    </li>
                    <li>
                        <a class="next" id="contacts_Next"></a>
                    </li>
                </ul>
            </div>
        </div>

        <!--联系人表单-->
        <div id="contactFrom">
            @{ Html.RenderAction("ContactForm", "UserCenter"); }
        </div>
    </div>
</div>
